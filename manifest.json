{
  "manifest_version": 2,
  "name": "Rokhdad",
  "version": "1.0",
  "description": "Rokhdad",
  "permissions": ["activeTab", "storage"],
  "icons": {
    "48": "128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.eseminar.tv/*", "*://eseminar.tv/*"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": {
      "48": "128.png"
    },
    "default_popup": "popup.html"
  }
}
